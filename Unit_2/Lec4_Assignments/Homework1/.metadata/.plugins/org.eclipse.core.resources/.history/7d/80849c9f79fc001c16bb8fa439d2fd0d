#include <stdio.h>
#include <stdlib.h>

int main(){

	float m_a [2][2];
	float m_b [2][2];

	int i =0;
	int j =0;

	for(i = 0; i < 2; ++i){
		printf("Enter elements of 1st matrix");
		fflush(stdout);
		for(i = 0; i < 2; ++i){
			printf("Enter a%d%d: ",(i+1),(j+1));
			fflush(stdout);fflush(stdin);
			scanf("%f",&m_a[i][j]);
			}
	}



	return 0;
}

