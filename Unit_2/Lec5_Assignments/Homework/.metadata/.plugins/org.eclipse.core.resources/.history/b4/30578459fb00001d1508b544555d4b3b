#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void printReverse(char text []);

int main(){

	printReverse("AHMED MOStafa");

	return 0;
}

void printReverse(char text []){

	if(text[0] != '\0'){
		printReverse(text +1);
		printf("%c",text[0]);
	}
}
