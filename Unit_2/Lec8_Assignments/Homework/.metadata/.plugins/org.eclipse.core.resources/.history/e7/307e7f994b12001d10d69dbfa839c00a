#include <stdio.h>

struct Semployee{
	char name [20];
	int id;
};

struct Semployee person = {"Alex",1002};

int main(){

	struct Semployee* ptr = &person;

	printf("Employee Name : %s\n",ptr->name);
	fflush(stdout);
	printf("Employee ID : %d",ptr->id);


	return 0;
}
