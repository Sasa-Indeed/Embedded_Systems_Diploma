#include <stdio.h>
#include<stdlib.h>

int main(){
	float x,y,z;
	printf("Enter three integers: ");
	fflush(stdout);
	scanf("%f%f%f",&x,&y,%z);
	fflush(stdout);fflush(stdin);

	if((x%2) == 0){
		printf("%d is even.",x);
	}else{
		printf("%d is odd.",x);
	}

	return 0;
}

